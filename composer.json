{
  "name": "b13/masi",
  "type": "typo3-cms-extension",
  "authors": [
    {
      "name": "Benni Mack",
      "email": "benjamin.mack@b13.com",
      "role": "Developer",
      "homepage": "https://github.com/bmack/"
    }
  ],
  "description": "TYPO3 Extend URL generation to optionally include storage folders.",
  "homepage": "https://github.com/b13/masi",
  "license": ["GPL-2.0-or-later"],
  "require": {
    "php": "~8.1",
    "typo3/cms-core": "^12.4 || ^13.0"
  },
  "replace": {
    "typo3-ter/masi": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "masi"
    }
  },
  "keywords": [
    "typo3",
    "slug",
    "url"
  ],
  "support": {
    "issues": "https://github.com/b13/masi/issues"
  },
  "autoload": {
    "psr-4": {
      "B13\\Masi\\": "Classes/"
    }
  }
}
